{
    "name": "alexandresalome/terminal",
    "description": "A library to manipulate terminals",
    "license": "MIT",
    "type": "library",

    "autoload": {
        "psr-4": {
            "Terminal\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Terminal\\Dev\\": "src_dev/",
            "Terminal\\Tests\\": "tests/"
        }
    },

    "require-dev": {
        "symfony/error-handler": "^5.2",
        "symfony/var-dumper": "^5.2",
        "phpunit/phpunit": "^9.5"
    },

    "extra": {
        "branch-version": "0.1-dev"
    },
  "require": {
    "ext-pcntl": "*"
  }
}
